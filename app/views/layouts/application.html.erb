<!DOCTYPE html>
<html>
<head>
  <title>DanielHarrisWeek7</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if current_user %>
    <h1><%= current_user.user_name %> | </h1>
    <%= link_to 'Signout', signout_path %>
  <% else %>
    <%= link_to 'Signin', signin_path %>
  <% end %>

  <%= yield %>

</body>
</html>
